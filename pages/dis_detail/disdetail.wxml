<view class='container'>
    <view class='list'>
    <view class='listTitle'>
      <text>分销用户</text>
    </view>
    <view class='listInfo'>
      <view class='listItem' wx:for="{{detail}}" wx:key="*this">
        <view>
          <image src='{{item.goods_img[0].goods_img}}'></image>
          <text class='goods'>{{item.goods_name}}</text>
        </view>
        <view>
          <text class='money'>{{item.pay_time}}</text>
        </view>
      </view>
    </view>
  </view>
</view>